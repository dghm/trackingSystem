//- Domestic (Completed) Timeline Preview
.timeline-preview.timeline-preview--domestic-completed(data-variant="domestic-completed")
  .timeline-visual.timeline-visual--domestic
    .timeline-track
    .timeline-connector
    .timeline-nodes-container
      .timeline-node.completed(data-step="1")
        .node-date
          span.month Nov
          span.day 10
        .node-icon
          .node-circle
        .node-info
          .node-status Picked Up
          p.node-time 09:20
      .timeline-node.processing(data-step="2")
        .node-date
          span.month Nov
          span.day 10
        .node-icon
          .node-circle
        .node-info
          .node-status In Transit
          p.node-time 14:05
      .timeline-node.processing(data-step="3")
        .node-date
          span.month Nov
          span.day 10
        .node-icon
          .node-circle
        .node-info
          .node-status Customs Clearance
          p.node-time 18:40
      .timeline-node.pending(data-step="4")
        .node-date
          span.month Nov
          span.day 11
        .node-icon
          .node-circle
        .node-info
          .node-status Out for Delivery
          p.node-time Scheduled

